{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "filesGlob": [
    "index.ts",
    "bin/dts-generator.ts",
    "typings/**/*.d.ts",
    "!typings/browser/**/*.d.ts",
    "!typings/browser.d.ts",
    "!typings/main.d.ts",
    "node_modules/typescript/lib/lib.es7.d.ts"
  ],
  "files": [
    "typings/main/ambient/bluebird/bluebird.d.ts",
    "typings/main/ambient/mkdirp/mkdirp.d.ts",
    "typings/main/ambient/node/node.d.ts",
    "node_modules/typescript/lib/lib.es7.d.ts",
    "index.ts",
    "bin/dts-generator.ts"
  ]
}
